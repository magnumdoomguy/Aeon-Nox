<?xml version="1.0" encoding="UTF-8"?>
<includes>

	<!-- Label for Home Videos Icon Style Setting -->
	<variable name="Advanced_HomeVideosIconLabelVar">
		<value condition="Skin.HasSetting(Advanced.PortraitHomeVideos)">$LOCALIZE[42406]</value> <!-- STRING: Portrait -->
		<value>$LOCALIZE[31076]</value> <!-- STRING: Landscape -->
	</variable>

	<!-- Label for Background Rotation Speed -->
	<variable name="FanartTimePerImageLabelVar">
		<value condition="!IsEmpty(Skin.String(FanartTimePerImage))">$INFO[Skin.String(FanartTimePerImage)] $LOCALIZE[31090]</value>
		<value>7 $LOCALIZE[31090]</value>
	</variable>

	<!-- Preplay Extra Transparency -->
	<variable name="PreplayTransVar">
		<value condition="StringCompare(Skin.String(Preplay.Trans),90)">EEFFFFFF</value>
		<value condition="StringCompare(Skin.String(Preplay.Trans),80)">DDFFFFFF</value>
		<value condition="StringCompare(Skin.String(Preplay.Trans),70)">CCFFFFFF</value>
		<value condition="StringCompare(Skin.String(Preplay.Trans),60)">BBFFFFFF</value>
		<value condition="StringCompare(Skin.String(Preplay.Trans),50)">AAFFFFFF</value>
		<value condition="StringCompare(Skin.String(Preplay.Trans),40)">99FFFFFF</value>
		<value condition="StringCompare(Skin.String(Preplay.Trans),30)">88FFFFFF</value>
		<value condition="StringCompare(Skin.String(Preplay.Trans),20)">77FFFFFF</value>
		<value condition="StringCompare(Skin.String(Preplay.Trans),10)">66FFFFFF</value>
		<value>FFFFFFFF</value>
	</variable>	

	<!-- Preplay Extra Transparency Setting -->
	<variable name="PreplayTransSettingVar">
		<value condition="IsEmpty(Skin.String(Preplay.Trans))">Skin.SetString(Preplay.Trans,90)</value>
		<value condition="StringCompare(Skin.String(Preplay.Trans),90)">Skin.SetString(Preplay.Trans,80)</value>
		<value condition="StringCompare(Skin.String(Preplay.Trans),80)">Skin.SetString(Preplay.Trans,70)</value>
		<value condition="StringCompare(Skin.String(Preplay.Trans),70)">Skin.SetString(Preplay.Trans,60)</value>
		<value condition="StringCompare(Skin.String(Preplay.Trans),60)">Skin.SetString(Preplay.Trans,50)</value>
		<value condition="StringCompare(Skin.String(Preplay.Trans),50)">Skin.SetString(Preplay.Trans,40)</value>
		<value condition="StringCompare(Skin.String(Preplay.Trans),40)">Skin.SetString(Preplay.Trans,30)</value>
		<value condition="StringCompare(Skin.String(Preplay.Trans),30)">Skin.SetString(Preplay.Trans,20)</value>
		<value condition="StringCompare(Skin.String(Preplay.Trans),20)">Skin.SetString(Preplay.Trans,10)</value>
		<value condition="StringCompare(Skin.String(Preplay.Trans),10)">Skin.Reset(Preplay.Trans)</value>
	</variable>

	<!-- Preplay Transparency Label -->
	<variable name="PreplayTransLabelVar">
		<value condition="!IsEmpty(Skin.String(Preplay.Trans))">$INFO[Skin.String(Preplay.Trans)]</value>
		<value>100</value>
	</variable>

	<!-- Colour Set Label -->
	<variable name="ColourPaletteLabelVar">
		<value condition="StringCompare(Skin.String(Colour.Palette),rainbow)">$LOCALIZE[42267]</value> <!-- STRING: rainbow -->
		<value condition="StringCompare(Skin.String(Colour.Palette),material-design)">$LOCALIZE[42269]</value> <!-- STRING: material-design -->
		<value>$LOCALIZE[42270]</value> <!-- STRING: basic -->
	</variable>

	<!-- Colour Set Setting -->
	<variable name="ColourPaletteSettingVar">
		<value condition="IsEmpty(Skin.String(Colour.Palette))">Skin.SetString(Colour.Palette,rainbow)</value>
		<value condition="StringCompare(Skin.String(Colour.Palette),rainbow)">Skin.SetString(Colour.Palette,material-design)</value>
		<value condition="StringCompare(Skin.String(Colour.Palette),material-design)">Skin.Reset(Colour.Palette)</value>
	</variable>

	<!-- Font Label -->
	<variable name="FontLabelVar">
		<value condition="IsEmpty(Skin.String(MainMenu.Font))">Default (Skin or Theme)</value> 
		<value>$INFO[Skin.String(MainMenu.Font)]</value>
	</variable>

	<!-- Font Setting -->
	<variable name="FontSettingVar">
		<value condition="IsEmpty(Skin.String(MainMenu.Font))">Skin.SetString(MainMenu.Font,Linear)</value>
		<value condition="StringCompare(Skin.String(MainMenu.Font),Linear)">Skin.SetString(MainMenu.Font,Open Sans)</value>
		<value condition="StringCompare(Skin.String(MainMenu.Font),Open Sans)">Skin.SetString(MainMenu.Font,Source Sans)</value>
		<value condition="StringCompare(Skin.String(MainMenu.Font),Source Sans)">Skin.SetString(MainMenu.Font,Trueno)</value>
		<value condition="StringCompare(Skin.String(MainMenu.Font),Trueno)">Skin.Reset(MainMenu.Font)</value>	
	</variable>
		
	<variable name="ColourSwatch">
		<value>$INFO[Container(3110).ListItem.Label]</value>
	</variable>

	<variable name="Homemenu_OrientationVar">
		<value condition="!Skin.HasSetting(Homemenu.Vertical)">$LOCALIZE[42184]</value>
		<value>$LOCALIZE[42183]</value>
	</variable>

	<variable name="middlemainmenuVar">
		<value condition="!Skin.HasSetting(middlemainmenu)">$LOCALIZE[42079]</value>
		<value>$LOCALIZE[42080]</value>
	</variable>

	<variable name="HorizontalListNavVar">
		<value condition="Skin.HasSetting(HorizontalListNav)">$LOCALIZE[31005]</value>
		<value>$LOCALIZE[571]</value>
	</variable>

	<variable name="MainHorizontalListNavVar">
		<value condition="Skin.HasSetting(MainHorizontalListNav)">$LOCALIZE[31005]</value>
		<value>$LOCALIZE[571]</value>
	</variable>

	<variable name="Homemenu_InfobarPositionVar">
		<value condition="Skin.HasSetting(Homemenu.LowerInfoBar)">$LOCALIZE[42079]</value>
		<value>$LOCALIZE[42189]</value>
	</variable>

	<!-- Number of Items on Horizontal Menu -->
	<variable name="MainMenuLayoutVar">
		<value condition="StringCompare(Skin.String(MainMenu.Layout),1 Item)">$LOCALIZE[42185]</value>
		<value condition="StringCompare(Skin.String(MainMenu.Layout),5 Items)">$LOCALIZE[42187]</value>
		<value>$LOCALIZE[42186]</value>
	</variable>

	<!-- Number of Items on Horizontal Menu Setting -->
	<variable name="MainMenuLayoutSettingVar">
		<value condition="IsEmpty(Skin.String(MainMenu.Layout))">Skin.SetString(MainMenu.Layout,5 Items)</value>
		<value condition="StringCompare(Skin.String(MainMenu.Layout),5 Items)">Skin.SetString(MainMenu.Layout,1 Item)</value>
		<value condition="StringCompare(Skin.String(MainMenu.Layout),1 Item)">Skin.Reset(MainMenu.Layout)</value>
	</variable>

	<!-- Search Options Label -->
	<variable name="SearchLabelVar">
		<value condition="StringCompare(Skin.String(MainMenu.Search),Disabled)">$LOCALIZE[42093]</value> <!-- STRING: Disabled -->
		<value>$LOCALIZE[42209]</value> <!-- STRING: Homemenu -->
	</variable>

	<!-- Search Options Setting -->
	<variable name="SearchSettingVar">
		<value condition="IsEmpty(Skin.String(MainMenu.Search))">Skin.SetString(MainMenu.Search,Disabled)</value>
		<value condition="StringCompare(Skin.String(MainMenu.Search),Disabled)">Skin.Reset(MainMenu.Search)</value>
	</variable>

	<!-- Home Menu Graphic Label -->
	<variable name="Homemenu_BarBackgroundLabelVar">
		<value condition="StringCompare(Skin.String(MainMenu.Graphic),Black Leaf)">$LOCALIZE[42202]</value>
		<value condition="StringCompare(Skin.String(MainMenu.Graphic),Glass)">$LOCALIZE[42056]</value>
		<value condition="StringCompare(Skin.String(MainMenu.Graphic),Mesh)">$LOCALIZE[42203]</value>
		<value>$LOCALIZE[31390]</value>
	</variable>

	<!-- Home Menu Graphic Setting -->
	<variable name="Homemenu_BarBackgroundSettingVar">
		<value condition="IsEmpty(Skin.String(MainMenu.Graphic))">Skin.SetString(MainMenu.Graphic,Black Leaf)</value>
		<value condition="StringCompare(Skin.String(MainMenu.Graphic),Black Leaf)">Skin.SetString(MainMenu.Graphic,Glass)</value>
		<value condition="StringCompare(Skin.String(MainMenu.Graphic),Glass)">Skin.SetString(MainMenu.Graphic,Mesh)</value>
		<value condition="StringCompare(Skin.String(MainMenu.Graphic),Mesh)">Skin.Reset(MainMenu.Graphic)</value>
	</variable>

	<!-- Hack to Allow the Number 4 to be Displayed in the Watched Status Indicator Settings without being Localized -->
	<variable name="TVSample1Var">
		<value>4</value>
	</variable>

